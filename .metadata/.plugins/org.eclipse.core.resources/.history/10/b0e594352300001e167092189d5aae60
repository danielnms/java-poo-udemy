import java.util.Scanner;

public class aula_26_parte2 {

	public static void main(String[] args) {
		
		// uso do sc.nextLine
		
		Scanner sc = new Scanner(System.in);
		
			String s1, s2, s3;
			
			s1 = sc.next();
			s2 = sc.nextLine();
			s3 = sc.nextLine();
			
			System.out.printf("Dados digitados:%n %d %d %d", s1, s2, s3);
			
		sc.close();
	}

}
